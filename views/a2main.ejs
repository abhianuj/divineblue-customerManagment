<!DOCTYPE html>
<html>
    <head>
        <link rel="stylesheet" type="text/css" href="../assets/a2mstyle.css">
        <title>Client</title>
    </head>
    
    <body>
        
        <!--left-->
        <div id="left">
            <img id="image" src="data:image/svg+xml;utf8;base64," />
            <br>View client
        </div>

        <!--top menu-->
        <div id="top">
            Divine Blue
        </div>

        <!--dashboard-->
        <div id="dash">
            <h1>Details of Microsoft:</h1>
            <table>
                <tr>
                    <td id="srno">srno: </td>
                    <td><%= data.srno %></td>
                </tr>
                <tr>
                    <td id="compname">Company name: </td>
                    <td><%= data.compname %></td>
                </tr>
                <tr>
                    <td id="clientname">Client name: </td>
                    <td><%= data.clientname %></td>
                </tr>
                <tr>
                    <td id="reference">Reference: </td>
                    <td><%= data.reference %></td>
                </tr>
                <tr>
                    <td id="contact">Contact number: </td>
                    <td><%= data.contact %></td>
                </tr>
                <tr>
                    <td id="email">Email ID: </td>
                    <td><%= data.email %></td>
                </tr>
                <tr>
                    <td id="standard">standard: </td>
                    <td><%= data.standard %></td>
                </tr>
                <tr>
                    <td id="certno">Certification number: </td>
                    <td><%= data.certno %></td>
                </tr>
                <tr>
                    <td id="issuedate">Issue Date: </td>
                    <td><%= data.issuedate %></td>
                </tr>
                <tr>
                    <td id="surv1">1<sup>st</sup> Surveillence: </td>
                    <td><%= data.surv1 %></td>
                </tr>
                <tr>
                    <td id="surv2">2<sup>nd</sup> Surveillence:</td>
                    <td><%= data.surv2 %></td>
                </tr>
                <tr>
                    <td id="certexp">Certfication Expiry: </td>
                    <td><%= data.certexp %></td>
                </tr>
                <tr>
                    <td id="country">Country: </td>
                    <td><%= data.country %></td>
                </tr>
                <tr>
                    <td id="cb">CB: </td>
                    <td><%= data.cb %></td>
                </tr>
            </table>
        </div>

    </body>
</html>