<!DOCTYPE html>
<html>
    <head>
        <link rel="stylesheet" type="text/css" href="assets/a4style.css">
        <title>Customers</title>
    </head>
    
    <body>
        
        <!--left-->
        <div id="left">
            <img id="image" src="data:image/svg+xml;utf8;base64," />
            <br>Manipulate certification expiry of clients
        </div>

        <!--top menu-->
        <div id="top">
            Divine Blue
        </div>

        <!--dashboard-->
        <div id="dash">
            
            <form action="/a4" method="post">
                <label>Input the date to evaluate for expiry: <input type="date" name="datec" required="require">
                    <input type="submit" value="check">
                    <br>
                    <br>
                </label>
            </form>
            <br>
            <% if(data2==true){ %>
            <div id="dates">The clients that have certification expiry before <span> <%=data1.split("-").reverse().join("-") %> </span> are:</div>
            <br>
            <% } %>
            
            <div id="header">
                <div class="srno">srno</div><div class="compname">Company name</div><div class="clientname">Client name</div><div class="contact">Contact no.</div><div class="issuedate">issuedate</div><div class="certexp">Certification expiry</div>
            </div>
            <% for(let i=0;i<data.length;i++){ %>
            <a id="customer" href="a2/<%= data[i].srno %>">
                <div class="srno"><%= data[i].srno %></div><div class="compname"><%= data[i].compname %></div><div class="clientname"><%= data[i].clientname %></div><div class="contact"><%= data[i].contact %></div><div class="issuedate"><%= data[i].issuedate %></div><div class="certexp"><%= data[i].certexp %></div>
            </a>        
            <% } %>
            
        </div>

    </body>
</html>